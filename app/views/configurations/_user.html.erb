<li class="row-fluid" id="<%= user.id %>">
  <div class="span4">
    <i class="active-user b icon-<%= user.link_active? %> icon-large" title="Active"></i>
    <%= user.master_account_tag %>
    <%= user %>
    <%= user.logged_tag %>
  </div>
  <div class="span4">
    <%= user.email %>
  </div>
  <div class="span2">
    <%= user.role %>
  </div>
  <div class="span2">
    <%= link_to icon_edit, edit_admin_user_path(user), class: 'dark' %>
    <%= user.delete_link %>
    <%= user.activate_deactivate_link %>
  </div>
</li>
